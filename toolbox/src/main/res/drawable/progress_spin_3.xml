<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">


    <item android:left="4dp" android:top="4dp"
        android:right="4dp" android:bottom="4dp">

        <rotate android:fromDegrees="-75">

            <shape android:shape="oval">

                <size android:width="13dp" android:height="13dp"/>
                <stroke android:width="2dp" android:color="@color/color_blue"
                    android:dashWidth="26dp" android:dashGap="8dp"/>

            </shape>
        </rotate>
    </item>


    <item>
        <rotate android:fromDegrees="75">

            <shape android:shape="oval">

                <size android:width="26dp" android:height="26dp"/>
                <stroke android:width="2dp" android:color="@color/color_blue"
                    android:dashWidth="52dp" android:dashGap="16dp"/>

            </shape>
        </rotate>
    </item>

</layer-list>